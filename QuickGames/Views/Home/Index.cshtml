<div ng-app="gameGrid">
	Sequenced Grid
	<div ng-controller="sequencedGridController as gridViewModel">
		<table style="width:100%">
			<tr ng-repeat="row in gridViewModel.grid.rows">
				<td ng-repeat="cell in row" class="btn btn-primary" style="width:50px;">{{cell.Value}}</td>
			</tr>
		</table>
	</div>	
</div>

